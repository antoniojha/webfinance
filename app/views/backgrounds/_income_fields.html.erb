<tbody id=<%="income_field_#{builder.index}"%>>
  <tr class="blank_row"><td></td><td></td></tr>
  <tr>
    <td><div class="title"><%="Income #{builder.index+1}" %></div></td>
    <td></td>     
  </tr>
  <tr>
    <td><%=builder.label :description%></td>
    <td><%=builder.text_field :description, :class=>"form-control"%></td>
  </tr>
  <tr>
    <td><%=builder.label :amount%></td>
    <td><%=builder.text_field :amount, :class=>"form-control"%></td>
  </tr>
  <tr>
    <td><%=builder.label :category%></td>
    <td><%=builder.select :category, Order::INCOME_TYPES, {prompt:"what type of income?"},{:class=>"form-control"}%>
    </td>
  </tr>
  <tr>
    <td><%=builder.hidden_field :_destroy%></td>
    <% if builder.index==0%>
    <td align="right"></td>
    <%else%>
    <td align="right"><%=render partial:'remove_link', locals:{name:"remove income"}%></td>
    <%end%>
  </tr>
</tbody>