<% unless @error==true%>
$("#profile_infos").html("<%=escape_javascript render(partial:'brokers/profile/profile_infos',locals:{mode:"personal"})%>");
$("#abouts").html("<%=escape_javascript render(partial:'brokers/profile/abouts',locals:{mode:"personal"})%>");
$("#skills").html("<%=escape_javascript render(partial:'brokers/profile/skills',locals:{mode:"personal"})%>");
$("#licenses").html("<%=escape_javascript render(partial:'brokers/profile/licenses',locals:{mode:"personal"})%>");
$("#financial_vehicles").html("<%=escape_javascript render(partial:'brokers/profile/financial_vehicles',locals:{mode:"personal"})%>");
$("#experiences").html("<%=escape_javascript render(partial:'brokers/profile/experiences',locals:{mode:"personal"})%>");
$("#financial_vehicle_dialog_form").dialog("close");
$("#financial_vehicle_dialog_form").html("<%=escape_javascript render('brokers/edit/financial_vehicle_edit')%>");
$("#fin_product_add_dialog_form").html("<%=escape_javascript render 'brokers/modal/fin_product_add_form'%>");

<%else%>
$("#profile_infos_error_msg").html("<%=escape_javascript render partial:'brokers/profile/error', locals:{object:@broker}%>");
$("#financial_vehicle_dialog_form #financial_vehicle_error_msg").html("<%=escape_javascript render partial:'brokers/profile/error', locals:{object:@broker}%>");
$("#financial_vehicle_dialog_form").scrollTop(0);

<%end%>

          