
<%@requests.each_with_index do|r,index|%>
<%broker=Broker.find(r.broker_id)%>
  <%if r.request_type=="create account"%>
    <%=render partial:"broker_application", locals:{r:r,broker:broker, index:index}%>

  <%elsif r.request_type=="create license"%>
    <%=render partial:"license_application", locals:{r:r,broker:broker, index:index}%>
  <%end%>

<%end%>
